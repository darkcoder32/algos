O(n^(2k+2))

void findMaximumNum(string str, int k, string& max)
{
    // Your code here
    if(k==0) return;
    int n=str.size();
    for(int i=0;i<n-1;i++){
        for(int j=i+1;j<n;j++){
            if(str[j]>str[i]){
                swap(str[i],str[j]);
                if(max<str){
                    max=str;
                }
                findMaximumNum(str,k-1,max);
                swap(str[i],str[j]);
            }
        }
    }
}
