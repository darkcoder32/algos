Time Complexity= O(n*m)

int lcs(vvi &dp,string s1,string s2,int n,int m){
    if(dp[n][m]!=-1) return dp[n][m];
    if(n==0 || m==0){
        return 0;
    }
    if(s1[n-1]==s2[m-1])
        dp[n][m]=1+lcs(dp,s1,s2,n-1,m-1);
    else{
        dp[n][m]=max(lcs(dp,s1,s2,n,m-1),lcs(dp,s1,s2,n-1,m));
    }
    return dp[n][m];
}
