Time Complexity  = O(n)

void inOrder(Node* root,int &ans,int x)
{
  // Your code here
  if(root==NULL) return;
  inOrder(root->left,ans,x);
  if(root->data<x){
      ans=root->data;
  }
  else if(root->data==x) ans=x;
  else return;
  inOrder(root->right,ans,x);
}
int floor(Node* root, int key) 
{ 
	int ans=INT_MAX;
	inOrder(root,ans,key);
	return ans;
}
