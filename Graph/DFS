Time Complexity = O(V+E)                   //also for disconnected graphs

dfsUtil(int s, vector<int> adj[], bool vis[]){
    if(vis[s]) return;
    vis[s]=1;
    cout<<s<<" ";
    for(int i=0;i<adj[s].size();i++){
        if(!vis[adj[s][i]]){
            dfs(adj[s][i],adj,vis);
        }
    }
}
void dfs(vector<int> adj[], bool vis[],int n)
{
    
    // Your code here
    for(int i=0;i<n;i++){
        dfsUtil(i,adj,vis);
    }
}
