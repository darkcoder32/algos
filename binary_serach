#include <bits/stdc++.h> 
using namespace std;


int binary(int arr[],int beg,int end,int x){
    if(end<beg){
        return -1;
    }
    int mid=beg+(end-beg)/2;
    if(arr[mid]==x){
        return mid;
    }
    else if(x<arr[mid]){
        binary(arr,beg,mid-1,x);
    }
    else if(x>arr[mid]){
        binary(arr,mid+1,end,x);
    }
}


int main(void) 
{ 
    
    int t;
    cin >> t;
    while(t--){
        int n, k;
        cin >> n;
        
        int arr[n];
        
        for(int i = 0;i<n;i++){
            cin >> arr[i];
        }
        cin>>k;
        
    }
	return 0; 
} 
